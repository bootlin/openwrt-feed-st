+++ b/arch/arm/boot/dts/st/stm32mp157a-ev1-scmi.dtsi
@@ -22,7 +22,7 @@ &cpu1 {
 };
 
 &dsi {
-	clocks = <&rcc DSI_K>, <&scmi_clk CK_SCMI_HSE>, <&rcc DSI_PX>;
