 CONFIG_RPMSG_CHAR=m
 CONFIG_RPMSG_CTRL=m
 CONFIG_RPMSG_TTY=m
@@ -41,6 +42,8 @@ CONFIG_SCHED_MC=y
 CONFIG_SERIAL_NONSTANDARD=y
 CONFIG_SRAM=y
 CONFIG_STM32_IPCC=y
+CONFIG_STM32_RPROC=y
+CONFIG_TEE_REMOTEPROC=y
